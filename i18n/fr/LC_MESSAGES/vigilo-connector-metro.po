#: src/twisted/plugins/vigilo_metro.py:44
msgid ""
"Please set the path to the configuration database generated by VigiConf "
"in the settings.ini."
msgstr ""
"Veuillez indiquer l'emplacement de la base de configuration générée par "
"VigiConf dans le fichier settings.ini."

#: src/vigilo/connector_metro/nodetorrdtool.py:132
#, python-format
msgid "Host '%(host)s' with datasource '%(ds)s' not found in the configuration !"
msgstr ""
"L'hôte '%(host)s' avec la source de données '%(ds)s' n'a pas été trouvé "
"dans le fichier de configuration !"

#: src/vigilo/connector_metro/nodetorrdtool.py:153
#, python-format
msgid "RRDtool could not create the file: %(filename)s. Message: %(msg)s"
msgstr "RRDtool n'a pas pu créer le fichier %(filename)s. Message : %(msg)s"

#: src/vigilo/connector_metro/nodetorrdtool.py:165
#, python-format
msgid "'%(msgtype)s' is not a valid message type for metrology"
msgstr "'%(msgtype)s' n'est pas un type de message valide pour la métrologie"

#: src/vigilo/connector_metro/nodetorrdtool.py:175
#, python-format
msgid "Not a valid performance message (missing '%(tag)s' tag)"
msgstr "Message de performance non valide (il manque la balise '%(tag)s')"

#: src/vigilo/connector_metro/nodetorrdtool.py:184
#, python-format
msgid "Invalid metrology value: %s"
msgstr "Valeur de métrologie non valide : %s"

#: src/vigilo/connector_metro/nodetorrdtool.py:190
#, python-format
msgid "Skipping perf update for host %s"
msgstr "Mise à jour de la donnée de performance sur l'hôte %s ignorée"

#: src/vigilo/connector_metro/nodetorrdtool.py:216
#, python-format
msgid "Unable to create the directory '%s'"
msgstr "Impossible de créer le dossier '%s'"

#: src/vigilo/connector_metro/nodetorrdtool.py:308
#, python-format
msgid "RRDtool could not update the file %(filename)s. Message: %(msg)s"
msgstr ""
"RRDtool n'a pas pu mettre à jour le fichier %(filename)s. Message : "
"%(msg)s"

#: src/vigilo/connector_metro/nodetorrdtool.py:408
msgid "Received signal to reload the configuration"
msgstr "Réception du signal de rechargement de la configuration"

#: src/vigilo/connector_metro/rrdtool.py:102
#, python-format
msgid "Unable to start \"%(rrdtool)s\". Make sure the path is correct."
msgstr ""
"Impossible de lancer \"%(rrdtool)s\". Assurez-vous que le chemin est "
"correct."

#: src/vigilo/connector_metro/rrdtool.py:105
#, python-format
msgid ""
"Unable to start \"%(rrdtool)s\". Make sure RRDtool is installed and you "
"have permissions to use it."
msgstr ""
"Impossible de démarrer \"%(rrdtool)s\". Assurez-vous que RRDtool est "
"installé et que vous disposez des permissions pour l'utiliser."

#: src/vigilo/connector_metro/rrdtool.py:118
#, python-format
msgid "The RRD directory does not exist: %s"
msgstr "Le dossier RRD n'existe pas : %s"

#: src/vigilo/connector_metro/rrdtool.py:127
#, python-format
msgid "Unable to create directory '%(dir)s'"
msgstr "Impossible de créer le dossier '%(dir)s'"

#: src/vigilo/connector_metro/rrdtool.py:131
#, python-format
msgid "Unable to write in the directory '%(dir)s'"
msgstr "Impossible d'écrire dans le dossier '%(dir)s'"

#: src/vigilo/connector_metro/rrdtool.py:196
#, python-format
msgid "Started RRDtool subprocess: pid %(pid)d"
msgstr "Processus de RRDtool démarré: pid %(pid)d"

#: src/vigilo/connector_metro/rrdtool.py:217
msgid "The process has not yet completed the previous job"
msgstr "Le process n'a pas encore fini le job précédent"

#: src/vigilo/connector_metro/rrdtool.py:250
msgid "No deferred available in _handle_result(), this should not happen"
msgstr "Aucun deferred disponible dans _handle_result(), ça ne devrait pas arriver"

#: src/vigilo/connector_metro/rrdtool.py:271
msgid "The RRDTool process exited normally"
msgstr "Le processus RRDTool a quitté normalement"

#: src/vigilo/connector_metro/rrdtool.py:273
#, python-format
msgid ""
"The RRDTool process was terminated abnormally with exit code %(rcode)s "
"and message: %(msg)s"
msgstr ""
"Le processus RRDTool s'est terminé anormalement avec le code %(rcode)s et"
" le message : %(msg)s"

#: src/vigilo/connector_metro/rrdtool.py:280
msgid "Restarting..."
msgstr "Redémarrage..."

#: src/vigilo/connector_metro/vigiconf_settings.py:51
msgid "No configuration database yet!"
msgstr "Aucune base de données de configuration pour le moment !"

