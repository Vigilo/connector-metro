#: src/vigilo/connector_metro/main.py:34
msgid ""
"Please set the path to the configuration file generated by VigiConf in "
"the settings.ini."
msgstr ""
"Veuillez indiquer l'emplacement du fichier de configuration généré par "
"VigiConf dans le fichier settings.ini."

#: src/vigilo/connector_metro/nodetorrdtool.py:81
msgid "Connection initialized"
msgstr "Connexion initialisée"

#: src/vigilo/connector_metro/nodetorrdtool.py:97
#, python-format
msgid "Unable to create directory '%(dir)s'"
msgstr "Impossible de créer le dossier '%(dir)s'"

#: src/vigilo/connector_metro/nodetorrdtool.py:101
#, python-format
msgid "Unable to write in the directory '%(dir)s'"
msgstr "Impossible d'écrire dans le dossier '%(dir)s'"

#: src/vigilo/connector_metro/nodetorrdtool.py:109
#, python-format
msgid "Started RRDtool subprocess: pid %(pid)d"
msgstr "Processus de RRDtool démarré: pid %(pid)d"

#: src/vigilo/connector_metro/nodetorrdtool.py:114
#, python-format
msgid ""
"Unable to start \"%(rrdtool)s\". Make sure RRDtool is installed and you "
"have permissions to use it."
msgstr ""
"Impossible de démarrer \"%(rrdtool)s\". Assurez-vous que RRDtool est installé "
"et que vous disposez des permissions pour l'utiliser."

#: src/vigilo/connector_metro/nodetorrdtool.py:122
#, python-format
msgid "RRDtool seemed to exit with return code %(returncode)d, restarting it..."
msgstr "RRDtool semble avoir quitté avec le code de retour %(returncode)d, redémarrage..."

#: src/vigilo/connector_metro/nodetorrdtool.py:150
#, python-format
msgid ""
"RRDtool choked on this command '%(cmd)s' using this file '%(filename)s'. "
"RRDtool replied with: '%(msg)s'"
msgstr ""
"RRDtool s'est bloqué sur cette commande '%(cmd)s' avec le fichier '%(filename)s'. "
"RRDtool a répondu: '%(msg)s'"

#: src/vigilo/connector_metro/nodetorrdtool.py:188
#, python-format
msgid "Unable to create the directory '%(dir)s'"
msgstr "Impossible de créer le dossier '%(dir)s'"

#: src/vigilo/connector_metro/nodetorrdtool.py:195
#, python-format
msgid ""
"Host '%(host)s' with datasource '%(ds)s' not found in the configuration "
"file (%(fileconf)s) !"
msgstr ""
"L'hôte '%(host)s' avec la source de données '%(ds)s' n'a pas été trouvé "
"dans le fichier de configuration ! (%(fileconf)s)"

#: src/vigilo/connector_metro/nodetorrdtool.py:229
#, python-format
msgid "'%(msgtype)s' is not a valid message type for metrology"
msgstr "'%(msgtype)s' n'est pas un type de message valide pour la métrologie"

#: src/vigilo/connector_metro/nodetorrdtool.py:238
#, python-format
msgid "Not a valid performance message (missing '%(tag)s' tag)"
msgstr "Message de performance non valide (il manque la balise '%(tag)s')"

#: src/vigilo/connector_metro/nodetorrdtool.py:260
#, python-format
msgid "Unable to create the directory '%s'"
msgstr "Impossible de créer le dossier '%s'"

#: src/vigilo/connector_metro/nodetorrdtool.py:284
#, python-format
msgid "Chat message to forward: %s"
msgstr "Message privé à relayer: %s"

#: src/vigilo/connector_metro/nodetorrdtool.py:327
msgid "Received signal to reload the configuration file"
msgstr "Réception du signal de rechargement du fichier de configuration"

#: src/vigilo/connector_metro/nodetorrdtool.py:332
msgid "Got exception"
msgstr "Réception d'une exception"
