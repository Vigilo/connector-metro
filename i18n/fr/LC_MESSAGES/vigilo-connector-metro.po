msgid   "Connection initialized"
msgstr  "Connexion initialisée"

#: src/vigilo/connector_metro/nodetorrdtool.py:91
#, python-format
msgid   "Unable to create directory '%(dir)s'"
msgstr  "Impossible de créer le répertoire '%(dir)s'"

#: src/vigilo/connector_metro/nodetorrdtool.py:95
#, python-format
msgid   "Unable to write in the directory '%(dir)s'"
msgstr  "Impossible d'écrire dans le répertoire '%(dir)s'"

#: src/vigilo/connector_metro/nodetorrdtool.py:103
#, python-format
msgid   "Started RRDtool subprocess: pid %(pid)d"
msgstr  "Sous-processus RRDtool démarré : pid %(pid)d"

#: src/vigilo/connector_metro/nodetorrdtool.py:108
#, python-format
msgid   "Unable to start \"%(rrdtool)s\". Make sure RRDtool is installed and "
        "you have permissions to use it."
msgstr  "Impossible de démarrer \"%(rrdtool)s\". Assurez-vous que RRDtool est "
        "installé et que vous disposez bien des droits nécessaires pour "
        "l'utiliser."

#: src/vigilo/connector_metro/nodetorrdtool.py:116
#, python-format
msgid   "RRDtool seemed to exit with return code %(returncode)d, restarting "
        "it..."
msgstr  "RRDtool semble s'être arrêté avec le code de retour %(returncode)d, "
        "redémarrage en cours."

#: src/vigilo/connector_metro/nodetorrdtool.py:144
#, python-format
msgid   "RRDtool choked on this command '%(cmd)s' using this file '%(filename)"
        "s'. RRDtool replied with: '%(msg)s'"
msgstr  "RRDtool a répondu par un message d'erreur à la commande '%(cmd)s' "
        "concernant le fichier '%(filename)s' : '%(msg)s'."

#: src/vigilo/connector_metro/nodetorrdtool.py:174
#, python-format
msgid   "Unable to create the directory '%(dir)s'"
msgstr  "Impossible de créer le répertoire '%(dir)s'"

#: src/vigilo/connector_metro/nodetorrdtool.py:180
#, python-format
msgid   "Host with this datasource '%(host_ds)s' not found in the "
        "configuration file (%(fileconf)s) !"
msgstr  "L'hôte correspondant au datasource '%(host_ds)s' est introuvable dans "
        "le fichier de configuration (%(fileconf)s) !"

#: src/vigilo/connector_metro/nodetorrdtool.py:211
#, python-format
msgid   "'%(msgtype)s' is not a valid message type for metrology"
msgstr  "'%(msgtype)s' n'est pas un message valide pour la metrologie"

#: src/vigilo/connector_metro/nodetorrdtool.py:223
#, python-format
msgid   "not a valid perf message (%(i)s is missing '%(perfmsg)s')"
msgstr  "Message de performance invalide (%(i)s est manquant '%(perfmsg)s')"

#: src/vigilo/connector_metro/nodetorrdtool.py:238
#, python-format
msgid   "Unable to create the directory '%s'"
msgstr  "Impossible de créer le répertoire '%s'"

#: src/vigilo/connector_metro/nodetorrdtool.py:259
#, python-format
msgid   "Chat message to forward: %s"
msgstr  "Message à transmettre : %s"

#: src/vigilo/connector_metro/nodetorrdtool.py:302
msgid   "Received signal to reload the configuration file"
msgstr  "Réception d'un signal de rechargement du fichier de configuration"

#: src/vigilo/connector_metro/nodetorrdtool.py:307
msgid   "Got exception"
msgstr  "Exception levée"

#: src/vigilo/connector_metro/main.py:34
msgid   "Please set the path to the configuration file generated by Vigiconf "
        "in the settings.ini."
msgstr  "Veuillez spécifier dans le fichier settings.ini le chemin vers le "
        "fichier de configuration fourni par Vigiconf."
